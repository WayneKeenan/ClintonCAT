{
  "manifest_version": 3,
  "name": "Clinton CAT",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "description": "Search Rossmann's Consumer Action Taskforce (CAT) wiki for the site being visited",
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}